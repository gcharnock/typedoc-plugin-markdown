{
  "name": "typedoc-plugin-markdown",
  "version": "0.0.11-0",
  "description": "A plugin for Typedoc that exposes themes and options for rendering markdown.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "copy:hbs": "cpx src/**/*.hbs dist/",
    "copy:pkg": "copyfiles package.json dist/index.js dist/**/*.hbs dist/**/*.js dist/theme/helpers/*.js node_modules/typedoc-plugin-markdown",
    "lint": "tslint --type-check --project tsconfig.json",
    "prepare": "tsc && npm run copy:hbs",
    "prepublishOnly": "rm -rf dist && npm run prepare",
    "samples": "typedoc --theme markdown --readme none --tsconfig samples/tsconfig.json --out samples/compiled --json tests/samples.json",
    "samples:html": "typedoc --readme none --tsconfig samples/tsconfig.json --out samples/compiledHtml",
    "pretest": "npm run prepare && npm run copy:pkg && npm run samples",
    "test": "mocha -r ts-node/register tests/tests.js",
    "watch": "concurrently \"npm run watch:tsc\" \"npm run watch:hbs\"",
    "watch:tsc": "tsc -w",
    "watch:hbs": "onchange 'src/**/*.hbs' -- npm run copy:hbs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git"
  },
  "keywords": [
    "typedocplugin",
    "typedoc"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "peerDependencies": {
    "typedoc": "^0.7.0"
  },
  "devDependencies": {
    "@types/chai": "4.0.0",
    "@types/mocha": "2.2.41",
    "chai": "4.0.2",
    "chai-files": "^1.4.0",
    "concurrently": "3.4.0",
    "copyfiles": "^1.2.0",
    "cpx": "1.5.0",
    "install": "^0.10.1",
    "mocha": "3.4.2",
    "npm": "^5.0.3",
    "onchange": "3.2.1",
    "ts-node": "^3.0.6",
    "tslint": "5.4.3",
    "typedoc": "0.7.1",
    "typescript": "2.3.4"
  }
}
